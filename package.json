{
  "name": "claude-code-collab",
  "version": "2.0.0",
  "description": "Local collaboration server enabling hidden team mode features in Claude Code",
  "type": "module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sethdford/claude-code-collab.git"
  },
  "keywords": [
    "claude-code",
    "multi-agent",
    "collaboration",
    "team-mode",
    "ai-agents",
    "orchestration"
  ],
  "author": "Seth Ford",
  "license": "MIT",
  "scripts": {
    "start": "tsx src/index.ts",
    "start:js": "node server.js",
    "dev": "tsx watch src/index.ts",
    "build": "tsc",
    "emulator": "firebase emulators:start",
    "patch": "node patch-cli.js",
    "patch:tasks": "node patch-tasks-only.js",
    "preflight": "./scripts/preflight.sh",
    "test": "./scripts/test-suite.sh",
    "e2e": "./scripts/e2e-test.sh",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "better-sqlite3": "^9.6.0",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.8",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.0",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.10",
    "eslint": "^8.57.0",
    "firebase-tools": "^13.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0"
  }
}
